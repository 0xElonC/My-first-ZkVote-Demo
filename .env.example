# Environment Variables for My First ZK Vote

# Optional: Analytics and tracking
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Optional: Custom RPC endpoints (if you want to use your own)
NEXT_PUBLIC_SEPOLIA_RPC=https://sepolia.infura.io/v3/YOUR_PROJECT_ID
NEXT_PUBLIC_MAINNET_RPC=https://mainnet.infura.io/v3/YOUR_PROJECT_ID

# Optional: WalletConnect Project ID (for better wallet support)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Optional: Custom contract addresses (if you deploy your own)
NEXT_PUBLIC_VOTING_CONTRACT_SEPOLIA=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_ZK_VOTING_CONTRACT_SEPOLIA=0x0987654321098765432109876543210987654321

# Development mode (set to "true" for additional debugging)
NEXT_PUBLIC_DEBUG_MODE=false

# Optional: IPFS gateway for metadata
NEXT_PUBLIC_IPFS_GATEWAY=https://gateway.pinata.cloud

# Optional: Error reporting (Sentry DSN)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here